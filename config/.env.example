TELEGRAM_BOT_TOKEN=your_token_here
ANTHROPIC_API_KEY=your_api_key_here
USER_TELEGRAM_ID=your_user_id_here
THINK_OS_PATH=/path/to/your/think-os

# User personalization (optional but makes Spark feel more human)
USER_NAME=your_name
USER_PRONOUNS=they/them  # or she/her, he/him
GUILT_LEVEL=medium  # chill, medium, or savage
SPARK_STYLE=ABC bro who roasts you  # Your preferred texting style

# Test mode: shorter intervals for testing
# TEST_MODE=true

# Orchestrator check interval (minutes) - random between min and max
# Default: 1-20 min in production, 10-20 sec in test mode
# TICK_MIN_MINUTES=1
# TICK_MAX_MINUTES=20

# Provider settings: anthropic (default) or deepseek
# ORCHESTRATOR_PROVIDER=anthropic
# COACH_PROVIDER=anthropic
# DEEPSEEK_API_KEY=your_deepseek_key_here

# Quiet hours - orchestrator won't nudge during these hours (24h format)
# Default: 11pm to 8am
# QUIET_START=23
# QUIET_END=8
